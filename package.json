{
    "name": "obsidian-timestamp-plugin",
    "version": "1.0.0",
    "description": "Automatically adds timestamps to new lines in notes with 'timestamps: true' frontmatter",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "timestamp"
    ],
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "builtin-modules": "^3.2.0",
        "esbuild": "^0.25.1",
        "obsidian": "latest",
        "tslib": "2.3.1",
        "typescript": "4.7.4"
    }
}
